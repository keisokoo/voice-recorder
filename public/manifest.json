{
  "manifest_version": 3,
  "name": "내 Chrome 확장 프로그램",
  "version": "1.0",
  "description": "React와 TypeScript로 만든 Chrome 확장 프로그램",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "fileSystem", "storage"],
  "file_system_provider_capabilities": {
    "configurable": true,
    "multiple_mounts": true,
    "source": "network"
  }
}
